{
  "name": "bun-temp",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "setup": "$npm_execpath install && cp -n .env.local.example .env.local",
    "fmt": "biome format --write .",
    "lnt": "biome lint --write .",
    "che": "biome check --write .",
    "tst": "echo 'vitest run してください'",
    "tst:w": "vitest watch --silent",
    "tst:ci": "vitest run --silent"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/bun": "latest",
    "husky": "^9.1.4",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "twitter-api-v2": "^1.17.2",
    "zod": "^3.23.8"
  }
}
