{
  "name": "ttddtdd",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "prepare": "husky",
    "setup": "$npm_execpath install && cp -n .env.local.example .env.local; $npm_execpath run db:i",
    "dev": "sls offline",
    "deploy": "sls deploy",
    "fmt": "biome format --write .",
    "lnt": "biome lint --write .",
    "che": "biome check --write .",
    "tst": "echo 'vitest run してください'",
    "tst:w": "vitest watch --silent",
    "tst:ci": "vitest run --silent",
    "db:i": "sls dynamodb install",
    "db:mig": "sls dynamodb migrate --stage local",
    "db:start": "sls dynamodb start --stage local",
    "db:admin": "dynamodb-admin -o"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.624.0",
    "@aws-sdk/lib-dynamodb": "^3.624.0",
    "@middy/core": "^3.4.0",
    "@middy/http-json-body-parser": "^3.4.0",
    "nodemailer": "^6.9.14",
    "twitter-api-v2": "^1.17.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.25",
    "@types/nodemailer": "^6.4.15",
    "dotenv": "^16.4.5",
    "dynamodb-admin": "^4.6.1",
    "esbuild": "^0.23.0",
    "husky": "^9.1.4",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-dynamodb": "^0.2.54",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^14.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
